{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Template API Types",
  "definitions": {
    "Template": {
      "properties": {
        "id": {
          "title": "Id",
          "type": "string"
        },
        "name": {
          "title": "Name",
          "type": "string"
        },
        "fields": {
          "items": {
            "type": "string"
          },
          "title": "Fields",
          "type": "array"
        },
        "prompt_template": {
          "anyOf": [{"type": "string"}, {"type": "null"}],
          "default": null,
          "title": "Prompt Template",
          "description": "Custom VLM prompt template with {{fields}} placeholder. Null means use default."
        }
      },
      "required": [
        "id",
        "name",
        "fields"
      ],
      "title": "Template",
      "type": "object"
    },
    "TemplateCreate": {
      "properties": {
        "name": {
          "title": "Name",
          "type": "string"
        },
        "fields": {
          "items": {
            "type": "string"
          },
          "title": "Fields",
          "type": "array"
        },
        "prompt_template": {
          "anyOf": [{"type": "string"}, {"type": "null"}],
          "default": null,
          "title": "Prompt Template",
          "description": "Custom VLM prompt template with {{fields}} placeholder. Null means use default."
        }
      },
      "required": [
        "name",
        "fields"
      ],
      "title": "TemplateCreate",
      "type": "object"
    },
    "TemplateUpdate": {
      "properties": {
        "name": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Name"
        },
        "fields": {
          "anyOf": [
            {
              "items": {
                "type": "string"
              },
              "type": "array"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Fields"
        },
        "prompt_template": {
          "anyOf": [{"type": "string"}, {"type": "null"}],
          "default": null,
          "title": "Prompt Template",
          "description": "Custom VLM prompt template with {{fields}} placeholder. Null means use default."
        }
      },
      "title": "TemplateUpdate",
      "type": "object"
    }
  }
}